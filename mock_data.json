{
  "/register": {
    "POST": {
      "$.request": {
        "$.body": {
          "name": "John Doe"
        }
      },
      "$.response": {
        "$.body": {
          "name": "John Doe",
          "age": 30,
          "address": {
            "street": "123 Main St",
            "city": "Anytown",
            "state": "CA",
            "zip": "12345"
          }
        }
      }
    }
  },
  "/video": {
    "GET": {
      "$.response": {
        "$.content-type": "video/mp4",
        "$.file": "./src/vid.mp4"
      }
    }
  },
  "/image": {
    "GET": {
      "$.response": {
        "$.content-type": "image/webp",
        "$.file": "https://mpost.io/wp-content/uploads/image-74-7.jpg"
      }
    }
  }
}
